# Example environment variables
# Copy this file to .env and fill in your credentials

# Sentinel Hub API
SENTINELHUB_CLIENT_ID=your-client-id-here
SENTINELHUB_CLIENT_SECRET=your-client-secret-here
SENTINELHUB_INSTANCE_ID=your-instance-id-here

# Copernicus Open Access Hub
COPERNICUS_USERNAME=your-username
COPERNICUS_PASSWORD=your-password

# USGS EarthExplorer (for Landsat)
USGS_USERNAME=your-username
USGS_PASSWORD=your-password

# Planet API (optional)
PLANET_API_KEY=your-api-key

# Application Settings
APP_DEBUG=false
LOG_LEVEL=INFO
